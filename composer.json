{
    "name": "nytris/bundle",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nytris\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nytris\\Bundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "asmblah/php-code-shift": "@dev",
        "nytris/boost": "@dev",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.2",
        "symfony/framework-bundle": "^5.0"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
